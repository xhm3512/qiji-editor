{
  "name": "dayTest",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "bin": {
    "my-cli": "bin.js"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress  --mode development --config  .deer/development.js",
    "start:mock": "cross-env NODE_ENV=development webpack-dev-server --progress  --mode development --config  .deer/development.js & npm run mock",
    "build": "cross-env NODE_ENV=production webpack --progress --mode production  --config  .deer/production.js",
    "build-prod": "cross-env NODE_ENV=production webpack --progress --mode production  --config  .deer/production.js",
    "build-test": "cross-env NODE_ENV=test webpack --progress --mode production  --config  .deer/production.js",
    "build-uat": "cross-env NODE_ENV=uat webpack --progress --mode production  --config  .deer/production.js",
    "dll": "webpack --progress --config .deer/dll.js --mode production",
    "analyze": "cross-env NODE_ANALYZE=analyze npm run build",
    "stats": "webpack --progress --config .deer/production.js --json > stats.json",
    "mock": "cybmock start",
    "commit": "git-cz",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint-staged": "lint-staged",
    "lint-staged:js": "",
    "precommit1": "eslint src/**/*.{tsx,js}",
    "dep": "./beta.sh && npm run genlog",
    "genlog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx  ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx  ./src && npm run lint:style",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "check-prettier lint"
  },
  "config": {
    "validate-commit-msg": {},
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less lint:style",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "git add"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ],
